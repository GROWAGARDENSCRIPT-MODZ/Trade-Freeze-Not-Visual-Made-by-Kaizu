local Rarities = {
	{ name = "Common",    chance = 58 },
	{ name = "Uncommon",  chance = 24 },
	{ name = "Rare",      chance = 12 },
	{ name = "Epic",      chance = 4  },
	{ name = "Legendary", chance = 1.5},
	{ name = "Mythic",    chance = 0.5},
}

local Colors = {
	Common    = Color3.fromRGB(200, 200, 200),
	Uncommon  = Color3.fromRGB(120, 220, 120),
	Rare      = Color3.fromRGB(120, 180, 255),
	Epic      = Color3.fromRGB(200, 120, 255),
	Legendary = Color3.fromRGB(255, 200, 90),
	Mythic    = Color3.fromRGB(255, 110, 110),
}

-- create a temporary egg near player
local function createFakeEgg()
	local char = player.Character or player.CharacterAdded:Wait()
	local root = char:WaitForChild("HumanoidRootPart")

	local egg = Instance.new("Part")
	egg.Name = "AdrianFakeEgg"
	egg.Shape = Enum.PartType.Ball
	egg.Material = Enum.Material.SmoothPlastic
	egg.Color = Color3.fromRGB(255, 245, 200)
	egg.Size = Vector3.new(2.2, 2.2, 2.2)
	egg.CanCollide = false
	egg.Anchored = true
	egg.CFrame = root.CFrame * CFrame.new(0, 3, -4)
	egg.Parent = workspace

	-- glow ring
	local ring = Instance.new("SelectionSphere")
	ring.SurfaceTransparency = 0.6
	ring.Transparency = 0.5
	ring.Color3 = Color3.fromRGB(140, 85, 255)
	ring.Adornee = egg
	ring.Parent = egg

	-- sparkles
	local pe = Instance.new("ParticleEmitter", egg)
	pe.Rate = 12
	pe.Speed = NumberRange.new(0.5, 1.5)
	pe.Lifetime = NumberRange.new(0.6, 1.2)
	pe.Size = NumberSequence.new({NumberSequenceKeypoint.new(0,0.2), NumberSequenceKeypoint.new(1,0)})
	pe.SpreadAngle = Vector2.new(15, 15)

	return egg
end

local function crackAnimation(egg, speedMult)
	speedMult = speedMult or 1
	local t1 = TweenService:Create(egg, TweenInfo.new(0.4/speedMult, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {Size = egg.Size * 1.15})
	local t2 = TweenService:Create(egg, TweenInfo.new(0.25/speedMult, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Size = egg.Size})
	t1:Play(); t1.Completed:Wait(); t2:Play(); t2.Completed:Wait()

	for i=1, 3 do
		local flick = TweenService:Create(egg, TweenInfo.new(0.12/speedMult), {Transparency = 0.4})
		flick:Play(); flick.Completed:Wait()
		egg.Transparency = 0
	end

	local shrink = TweenService:Create(egg, TweenInfo.new(0.45/speedMult, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {Size = Vector3.new(0.1,0.1,0.1), Transparency = 1})
	shrink:Play(); shrink.Completed:Wait()
end

-- confetti burst
local function confettiBurst(position, color)
	local p = Instance.new("Part")
	p.Anchored = true
	p.CanCollide = false
	p.Transparency = 1
	p.Size = Vector3.new(1,1,1)
	p.CFrame = CFrame.new(position + Vector3.new(0,2,0))
	p.Parent = workspace

	local pe = Instance.new("ParticleEmitter", p)
	pe.Rate = 0
	pe.EmissionDirection = Enum.NormalId.Top
	pe.Lifetime = NumberRange.new(0.8, 1.4)
	pe.Speed = NumberRange.new(8, 16)
	pe.VelocitySpread = 160
	pe.Rotation = NumberRange.new(0, 360)
	pe.RotSpeed = NumberRange.new(-80, 80)
	pe.Size = NumberSequence.new(0.25)
	pe.Color = ColorSequence.new(color)
	pe.Texture = "rbxassetid://243660364" -- small confetti texture
	pe:Emit(120)
	game:GetService("Debris"):AddItem(p, 2)
end

-- floating text
local function floatText(cf, text, color)
	local billboard = Instance.new("BillboardGui")
	billboard.Size = UDim2.fromOffset(220, 40)
	billboard.AlwaysOnTop = true
	billboard.StudsOffset = Vector3.new(0, 3, 0)
	billboard.Parent = workspace.Terrain

	local part = Instance.new("Part")
	part.Anchored = true
	part.CanCollide = false
	part.Transparency = 1
	part.Size = Vector3.new(1,1,1)
	part.CFrame = cf
	part.Parent = workspace

	billboard.Adornee = part

	local label = Instance.new("TextLabel", billboard)
	label.BackgroundTransparency = 1
	label.Size = UDim2.fromScale(1,1)
	label.Font = Enum.Font.GothamBlack
	label.TextSize = 24
	label.TextColor3 = color
	label.TextStrokeTransparency = 0.4
	label.Text = text

	local t = TweenService:Create(billboard, TweenInfo.new(1.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {StudsOffset = Vector3.new(0, 6, 0)})
	local f = TweenService:Create(label, TweenInfo.new(1.0), {TextTransparency = 1})
	t:Play(); task.delay(0.4, function() f:Play() end)
	game:GetService("Debris"):AddItem(part, 1.4)
	game:GetService("Debris"):AddItem(billboard, 1.5)
end

-- fake pet model simple orb w name
local function spawnFakePet(cf, petName, rarity)
	local sphere = Instance.new("Part")
	sphere.Name = "AdrianFakePet"
	sphere.Shape = Enum.PartType.Ball
	sphere.Material = Enum.Material.Neon
	sphere.Color = Colors[rarity] or Color3.fromRGB(255, 255, 255)
	sphere.Size = Vector3.new(1.7,1.7,1.7)
	sphere.CanCollide = false
	sphere.Anchored = true
	sphere.CFrame = cf * CFrame.new(0, 1.5, 0)
	sphere.Parent = workspace

	local bill = Instance.new("BillboardGui", sphere)
	bill.Size = UDim2.fromOffset(160, 36)
	bill.StudsOffset = Vector3.new(0, 2.4, 0)
	bill.AlwaysOnTop = true
	local lab = Instance.new("TextLabel", bill)
	lab.BackgroundTransparency = 1
	lab.Size = UDim2.fromScale(1,1)
	lab.Font = Enum.Font.GothamBold
	lab.TextSize = 18
	lab.TextColor3 = Colors[rarity] or Color3.new(1,1,1)
	lab.TextStrokeTransparency = 0.5
	lab.Text = petName .. "  " .. rarity .. "  client"

	-- idle bob
	task.spawn(function()
		local t = 0
		while sphere.Parent do
			t += task.wait()
			sphere.CFrame = cf * CFrame.new(0, 1.5 + math.sin(t*2)*0.2, 0)
		end
	end)

	game:GetService("Debris"):AddItem(sphere, 20)
end

-- pick rarity by weight
local function rollRarity()
	local total = 0
	for _,r in ipairs(Rarities) do total += r.chance end
	local pick = math.random() * total
	local acc = 0
	for _,r in ipairs(Rarities) do
		acc += r.chance
		if pick <= acc then
			return r.name
		end
	end
	return "Common"
end

local function animProgress(seconds)
	fill.Size = UDim2.new(0,0,1,0)
	local tw = TweenService:Create(fill, TweenInfo.new(seconds, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Size = UDim2.new(1,0,1,0)})
	tw:Play()
end

-- reveal banner
local revealed = false
local function revealPrank()
	if revealed then return end
	revealed = true
	title.Text = "Adrian Hatch â€¢ VISUAL ONLY"
	log("[WARN] server mode visual only no instance created")
	notify("pranked", "visual hatch lang walang real pet", 4)
end

-- main hatch pipeline
local function runHatch(times)
	times = times or 1
	local eggType = (#eggInput.Text > 0 and eggInput.Text) or "Common Egg"
	local targetPet = (#petInput.Text > 0 and petInput.Text) or "Kitsune"
	local spd = string.lower(spdInput.Text)
	local speedMult = (spd == "turbo" and 2.0) or (spd == "fast" and 1.4) or 1.0

	log(string.format("[INFO] egg=%s target=%s speed=%.1fx", eggType, targetPet, speedMult))
	log("[SEC] verifying client entitlements...")
	task.wait(0.4); log("[OK] ui pass")
	log("[NET] connecting hatch pipeline..."); task.wait(0.35)
	log("[OK] route=egg/hatch v3"); task.wait(0.25)

	for i=1, times do
		log(string.format("[ROLL] sequence %d", i))
		animProgress(0.9 / speedMult)
		local egg = createFakeEgg()
		crackAnimation(egg, speedMult)

		-- visual roll
		local rarity = rollRarity()
		local char = player.Character or player.CharacterAdded:Wait()
		local root = char:WaitForChild("HumanoidRootPart")
		local cf = root.CFrame * CFrame.new(0, 2.2, -4)

		confettiBurst(egg.Position, Colors[rarity] or Color3.new(1,1,1))
		floatText(cf, "HATCHED  " .. string.upper(targetPet) .. "  â€¢  " .. string.upper(rarity), Colors[rarity] or Color3.new(1,1,1))
		spawnFakePet(cf, targetPet, rarity)

		sysmsg(string.format("[%s] %s just hatched %s â€¢ %s  client",
			eggType, player.Name, targetPet, rarity))

		if egg and egg.Parent then egg:Destroy() end
		task.wait(0.25)
	end

	log("[DONE] hatch pipeline finished")
	-- auto reveal after some time para tumawa sila late
	task.delay(18, revealPrank)
end

hatchBtn.MouseButton1Click:Connect(function()
	hatchBtn.Text = "HATCHING..."
	hatchBtn.Active = false; multiBtn.Active = false
	runHatch(1)
	task.wait(1.0)
	hatchBtn.Text = "HATCH EGG"; hatchBtn.Active = true; multiBtn.Active = true
end)

multiBtn.MouseButton1Click:Connect(function()
	multiBtn.Text = "HATCHING x3..."
	hatchBtn.Active = false; multiBtn.Active = false
	runHatch(3)
	task.wait(1.0)
	multiBtn.Text = "HATCH x3"; hatchBtn.Active = true; multiBtn.Active = true
end)

UserInputService.InputBegan:Connect(function(i, gpe)
	if gpe then return end
	if i.KeyCode == Enum.KeyCode.H then
		revealPrank()
	end
end)

notify("Adrian Hatch", "loaded private build v2.4", 4)
log("[BOOT] ui attached ok")
log("[INFO] ready press H to reveal prank")
