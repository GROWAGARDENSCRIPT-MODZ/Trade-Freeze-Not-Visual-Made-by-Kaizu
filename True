		StarterGui:SetCore("SendNotification", {Title = t, Text = m, Duration = d or 3})
	end)
end

local function sysmsg(text)
	pcall(function()
		StarterGui:SetCore("ChatMakeSystemMessage", {
			Text = text,
			Color = Color3.fromRGB(255, 235, 140),
			Font = Enum.Font.SourceSansBold,
			TextSize = 18
		})
	end)
end

